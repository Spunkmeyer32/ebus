<?xml version="1.0" encoding="UTF-8" ?>
<settings xsi:schemaLocation='http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd'
	xmlns='http://maven.apache.org/SETTINGS/1.0.0' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'>

	<servers>
		<server>
			<id>bintray-csowada-mvn</id>
			<username>csowada</username>
			<password>${env.BINTRAY_TOKEN}</password>
		</server>
		<server>
			<id>snapshots</id>
			<username>csowada</username>
			<password>${env.JFROG_TOKEN}</password>
		</server>
	</servers>

	<profiles>
		<profile>
			<id>bintray</id>
		    <properties>
				<gpg.passphrase>${env.PASSPHRASE}</gpg.passphrase>
			</properties>
			
			<repositories>
				<repository>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<id>bintray-csowada-mvn</id>
					<name>bintray</name>
					<url>https://dl.bintray.com/csowada/mvn</url>
				</repository>
				
				        <repository>
          <snapshots />
          <id>snapshots</id>
          <name>libs-snapshot</name>
          <url>https://oss.jfrog.org/artifactory/libs-snapshot</url>
        </repository>
				
			</repositories>
			
			<pluginRepositories>
				<pluginRepository>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<id>bintray-csowada-mvn</id>
					<name>bintray-plugins</name>
					<url>https://dl.bintray.com/csowada/mvn</url>
				</pluginRepository>
				
				        <pluginRepository>
          <snapshots />
          <id>snapshots</id>
          <name>plugins-release</name>
          <url>https://oss.jfrog.org/artifactory/plugins-release</url>
        </pluginRepository>
				
			</pluginRepositories>
		</profile>
	</profiles>
	<activeProfiles>
		<activeProfile>bintray</activeProfile>
	</activeProfiles>
</settings>